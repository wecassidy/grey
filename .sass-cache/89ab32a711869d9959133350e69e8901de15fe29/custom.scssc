#wrapper {
	border: none;
	background: transparent;
}

/* Header */
#header {
	background: transparent;
	z-index: -1;
	border: none;
	border-bottom: 1px solid #999;
}

/* Navigation box */
#navigation_box {
	position: absolute;
	border: none;
	top: 50px;
	z-index: 1;
	left: 10%;
	width: 80%;
	background: transparent;
	color: transparent;
	padding: 0;

	@include vendorize(overflow-x, auto);
	a {
		color: $aColour;

		&:hover {
			color: $aHoverColour;
		}
